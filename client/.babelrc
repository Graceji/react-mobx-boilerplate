{
  "presets": [
    [
      "es2015",
      {
        "loose": true
      }
    ],
    "stage-0",
    "stage-1",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-runtime",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
